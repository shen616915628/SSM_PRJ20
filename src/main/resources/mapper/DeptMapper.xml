<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.senchen365.ssmempms.dao.DeptMapper">
    <select id="queryAll" resultType="com.senchen365.ssmempms.entity.DeptEntity">
        SELECT * from base_depts
    </select>
    <select id="queryByNo" parameterType="int" resultType="com.senchen365.ssmempms.entity.DeptEntity">
        SELECT * from base_depts where deptno=#{dno}
    </select>

</mapper>
